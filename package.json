    "concurrently": "^3.6.0",
    "core-js": "^3.8.2",
    "eslint": "^7.18.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.4.1",
    "lerna": "^4.0.0",
    "lint-staged": "^10.5.3",
